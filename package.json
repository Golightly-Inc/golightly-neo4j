{
  "name": "golightly-neo4j",
  "version": "0.0.1",
  "description": "The offical javascript client for Golightly's Neo4j server",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "author": "jjbeach <beachjoshua87@gmail.com>",
  "license": "MIT",
  "scripts": {
    "test": "jest --config jestconfig.json",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jjbeach/golightly-neo4j.git"
  },
  "keywords": [],
  "bugs": {
    "url": "https://github.com/jjbeach/golightly-neo4j/issues"
  },
  "homepage": "https://github.com/jjbeach/golightly-neo4j#readme",
  "devDependencies": {
    "@types/jest": "^25.1.1",
    "jest": "^25.1.0",
    "jest-fetch-mock": "^3.0.1",
    "prettier": "^1.19.1",
    "ts-jest": "^25.0.0",
    "tslint": "^6.0.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.7.5"
  },
  "files": [
    "lib/**/*"
  ]
}
